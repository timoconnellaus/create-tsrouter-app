{
  "name": "Store",
  "description": "Add TanStack Store to your application.",
  "phase": "add-on",
  "link": "https://tanstack.com/store/latest",
  "type": "add-on",
  "modes": ["file-router", "code-router"],
  "routes": [
    {
      "url": "/demo/store",
      "name": "Store",
      "path": "src/routes/demo.store.tsx",
      "jsName": "StoreDemo"
    }
  ],
  "integrations": [
    {
      "type": "devtools",
      "path": "src/lib/demo-store-devtools.tsx",
      "jsName": "StoreDevtools"
    }
  ]
}
